/* 活动列表页 */
.search {
  padding: 10rpx;
  height: 60rpx;
  background-color: paleturquoise;
  
  
}
.search input{
  background-color: #fff;
  display: flex;
  align-items: left;
  text-align: center;
  height: 100%;
  justify-content: center;
  border-radius: 15rpx;
  color: #666;
}
.inner {

width: 100%;


}
navigator{
  display: flex;
  border-bottom: 1rpx solid #ccc;
  margin: 30rpx;
  width: 100%;
  padding-bottom: 5rpx;
  height: 140rpx;
  

}

.inner .actives_img {
  flex: 2;
  display: flex;
  justify-content: center;
  align-items: center;

 
  margin-bottom: 6rpx;
}
.inner .actives_img image {
  width: 90%;
  border-radius: 20rpx;
  height: 140rpx;

  
}
.inner .actives_info {
  display: flex;
  flex: 3;
  flex-direction: column;
  justify-content: space-evenly;
}
.inner .actives_info .actives_name {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.inner .actives_info .actives_price {
  color: #dd3300;
  font-size: 20rpx;
}
