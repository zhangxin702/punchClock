.search_head {
  padding: 10rpx;
  height: 60rpx;
  background-color: paleturquoise;
  display: flex;
}
.search_head input{
  flex:1;
  background-color: #fff;
  /* display: flex; */
  text-align: center;
  height: 100%;
  /* justify-content: center; */
  border-radius: 15rpx;
  color: #666;
}
.search_head button{
  height: 100%;
  width: 150rpx;
  font-size:28rpx;
  padding: 0;
  margin: 0 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 15rpx;
}
.search_content {

  width: 100%;
  
}
navigator{
  display: flex;
  border-bottom: 1rpx solid #ccc;
  margin: 30rpx;
  width: 100%;
  padding-bottom: 5rpx;
  

}

.search_content .actives_img {
  flex: 2;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 6rpx;
}
.search_content .actives_img image {
  width: 90%;
  border-radius: 20rpx;
  height: 140rpx;
}
.search_content .actives_info {
  display: flex;
  flex: 3;
  flex-direction: column;
  justify-content: space-evenly;
}
.search_content .actives_info .actives_name {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.search_content .actives_info .actives_price {
  color: #dd3300;
  font-size: 20rpx;
}