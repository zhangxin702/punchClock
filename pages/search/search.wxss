.search_head {
  padding: 10rpx;
  height: 60rpx;
  background-color: paleturquoise;
  display: flex;
}
.search_head input{
  flex:1;
  background-color: #fff;
  /* display: flex; */
  text-align: center;
  height: 100%;
  /* justify-content: center; */
  border-radius: 15rpx;
  color: #666;
}
.search_head button{
  height: 100%;
  width: 150rpx;
  font-size:28rpx;
  padding: 0;
  margin: 0 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 15rpx;
}
.search_content .search_item{
  background-color: #fff;
  font-size: 26rpx;
  padding: 15rpx 10rpx;
  border-bottom: 1rpx solid #ccc;

  overflow: hidden;/*超出后隐藏 */
  white-space: nowrap;
  text-emphasis: ellipsis;/*超出部分显示... */
}